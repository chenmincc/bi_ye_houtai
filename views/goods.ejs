                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="updateName4">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="updateImg" class="col-sm-2 control-label">图片</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="updateImg">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="updateImg1" class="col-sm-2 control-label">大图片</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="updateImg1">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="updateImg2" class="col-sm-2 control-label">小图片1</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="updateImg2">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="updateImg3" class="col-sm-2 control-label">小图片2</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="updateImg3">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="updateImg4" class="col-sm-2 control-label">小图片3</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="updateImg4">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="updateImg5" class="col-sm-2 control-label">小图片4</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="updateImg5">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="updateName5" class="col-sm-2 control-label">快乐价</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="updateName5">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="updateName6" class="col-sm-2 control-label">原价</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="updateName6">
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                                <button type="button" class="btn btn-primary" id="bannerUpdate">确认修改</button>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- </div> -->

            </div>

        </div>

    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/js/bootstrap.min.js"></script>
